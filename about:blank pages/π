javascript:(function(){var win=window.open(''); win.document.head.innerHTML=' <title> %E2%A0%80 </title></head> '; win.document.body.innerHTML=' <body> [Enter to pause / unpause calculations] Pi: <div id="data" style="height: 350px; overflow:scroll; background-color: grey; padding:10px; color:white;"><span id="pi"></span></div> Total Digits: <span id="tot"></span> <br> Made with love by Cloverwave. Made with JS with the math ripped from here: https://rosettacode.org/wiki/Pi#Python </body> '; var script=document.createElement('script'); script.textContent=' var pi = document.getElementById("pi"); var tot = document.getElementById("tot"); function * calcpi(){ let q = 1n; let r = 0n; let t = 1n; let k = 1n; let n = 3n; let l = 3n; let four = 4n; let two = 2n; let ten = 10n; let seven = 7n; let one = 1n; let three = 3n; while (true){ if (four*q+r-t < n*t){ yield n; nr = ten*(r-(n*t)); n = (~~((ten*((three*q)+r))/t))-(ten*n); q  *= ten; r  = nr; } else { nr = (two*q+r)*l; nn = ~~(((q*(seven*k))+two+(r*l))/(t*l)); q  *= k; t  *= l; l  += two; k  += one; n  = nn; r  = nr; } } } var pi_digits = calcpi(); var dig; var total = 0; var stop = false; function digits(){ if (stop){ setTimeout(digits, 1); } else { tot.textContent = total; dig = pi_digits.next().value; pi.textContent += dig; total ++; pi.textContent += "\\n"; setTimeout(digits, 1); } } document.addEventListener("keydown", function(event) { if (event.code === "Enter") { if (stop){ stop = false; } else { stop = true; } } }); window.setInterval(function() { if (!stop){ var elem = document.getElementById("data"); elem.scrollTop = elem.scrollHeight; } }, 10); digits(); '; win.document.head.appendChild(script);var style=document.createElement('style');style.textContent=" ";win.document.head.appendChild(style);})();

